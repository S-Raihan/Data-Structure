#shafiullahRaihan
#SelectionSort program

def selectionSort(arr):
    for i in range(len(arr)):
        min_len = i
        for j in range(i+1,len(arr)):
            if arr[min_len]>arr[j]:
                min_len = j
                
        arr[i],arr[min_len] = arr[min_len], arr[i]
   
arr =[64, 25, 12, 22,11]   
selectionSort(arr)
for i in range(len(arr)):
    print(arr[i], end=" ")
    
    
#time complexity O(n^2)
    
