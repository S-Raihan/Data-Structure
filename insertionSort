#function for insertion array sort

def insertion(arr):
    for i in range(1,len(arr)):
        key = arr[i]
        j = i - 1
        while j>= 0 and key<arr[j]:
            arr[j+1] = arr[j]
            j-=1
        arr[j+1] = key


#array that we want to sort

arr = [3,2,66,44,6,677,45,233]
#demo array
insertion(arr)
for i in range(len(arr)):
    print(arr[i])









#time complexity O(n^2)
#space complexity O(1)
